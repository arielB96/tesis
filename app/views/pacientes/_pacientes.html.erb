<div class="center">
  <h1>Ficha Numero: <%= @ficha_num.numero %></h1>
</div>
<h1>Pacientes</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Apellido</th>
      <th>Ci</th>
      <th>Sexo</th>
      <th>Edad</th>
      <th>Lugarnacimiento</th>
      <th>Nacionalidad</th>
      <th>Etnia</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ficha_num.pacientes.each do |paciente| %>
      <tr>
        <td><%= paciente.nombre %></td>
        <td><%= paciente.apellido %></td>
        <td><%= paciente.ci %></td>
        <td><%= paciente.sexo %></td>
        <td><%= paciente.edad %></td>
        <td><%= paciente.lugarNacimiento %></td>
        <td><%= paciente.nacionalidad %></td>
        <td><%= paciente.etnia %></td>
      </tr>
  </tbody>
</table>
<% end %>