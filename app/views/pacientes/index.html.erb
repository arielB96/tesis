<p id="notice"><%= notice %></p>

<h1>Pacientes</h1>

<table class="table table-bordered">
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Apellido</th>
      <th>Ci</th>
      <th>Sexo</th>
      <th>Edad</th>
      <th>Lugarnacimiento</th>
      <th>Nacionalidad</th>
      <th>Etnia</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pacientes.each do |paciente| %>
      <tr>
        <td><%= paciente.nombre %></td>
        <td><%= paciente.apellido %></td>
        <td><%= paciente.ci %></td>
        <td><%= paciente.sexo %></td>
        <td><%= paciente.edad %></td>
        <td><%= paciente.lugarNacimiento %></td>
        <td><%= paciente.nacionalidad %></td>
        <td><%= paciente.etnia %></td>
        <td><%= link_to 'Show', paciente %></td>
        <td><%= link_to 'Edit', edit_paciente_path(paciente) %></td>
        <td><%= link_to 'Destroy', paciente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Paciente', new_paciente_path %>
