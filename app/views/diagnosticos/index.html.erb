<p id="notice"><%= notice %></p>

<h1>Diagnosticos</h1>

<table class="table table-bordered">
  <thead>
    <tr>
      <th>Fecha</th>
      <th>Hora</th>
      <th>Peso</th>
      <th>Nuevo</th>
      <th>Motivoconsul</th>
      <th>Diagnostico</th>
      <th>Examenfisico</th>
      <th>Otrodiagnos</th>
      <th>Tratamiento</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @diagnosticos.each do |diagnostico| %>
      <tr>
        <td><%= diagnostico.fecha %></td>
        <td><%= diagnostico.hora %></td>
        <td><%= diagnostico.peso %></td>
        <td><%= diagnostico.talla %></td>
        <td><%= diagnostico.nuevo %></td>
        <td><%= diagnostico.motivoConsul %></td>
        <td><%= diagnostico.diagnostico %></td>
        <td><%= diagnostico.examenFisico %></td>
        <td><%= diagnostico.otroDiagnos %></td>
        <td><%= diagnostico.tratamiento %></td>
        <td><%= link_to 'Show', diagnostico %></td>
        <td><%= link_to 'Edit', edit_diagnostico_path(diagnostico) %></td>
        <td><%= link_to 'Destroy', diagnostico, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Diagnostico', new_diagnostico_path %>
